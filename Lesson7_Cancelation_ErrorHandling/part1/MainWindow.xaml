<Window x:Class="part1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:part1"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <TextBox HorizontalAlignment="Left" Height="249" Margin="10,10,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="305" Name="txtInput"/>
        <TextBox HorizontalAlignment="Left" Height="259" Margin="468,0,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="314" Name="txtOutput"/>
        <Button Content="Execute" HorizontalAlignment="Left" Height="46" Margin="10,264,0,0" VerticalAlignment="Top" Width="305" Name="btnExecute" Click="btnExecute_Click"/>
        <Button Content="Cancel" HorizontalAlignment="Left" Height="46" Margin="468,264,0,0" VerticalAlignment="Top" Width="314" Name="btnCancel" Click="btnCancel_Click"/>
        <ProgressBar HorizontalAlignment="Left" Height="19" Margin="10,391,0,0" VerticalAlignment="Top" Width="772" Name="pbBar" Minimum="0" Maximum="100"/>

    </Grid>
</Window>
